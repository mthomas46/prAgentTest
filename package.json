{
  "name": "express-typescript-api",
  "version": "1.0.0",
  "description": "Express TypeScript API with OpenAPI documentation",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'NODE_OPTIONS=\"--loader ts-node/esm\" ts-node --esm' src/index.ts",
    "build": "tsc",
    "build:docs": "tsc -p tsconfig.docs.json",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "prepare": "husky install"
  },
  "keywords": [
    "express",
    "typescript",
    "api",
    "swagger"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/compression": "1.7.5",
    "@types/dotenv": "8.2.3",
    "@types/express-rate-limit": "6.0.2",
    "@types/helmet": "4.0.0",
    "@types/markdown-it": "14.1.2",
    "@types/swagger-jsdoc": "6.0.4",
    "@types/winston": "2.4.4",
    "compression": "1.8.0",
    "cors": "^2.8.5",
    "dotenv": "16.4.7",
    "express": "^4.18.2",
    "express-rate-limit": "7.5.0",
    "express-secure-headers": "1.0.0",
    "highlight.js": "11.11.1",
    "markdown-it": "14.1.0",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "3.17.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.14",
    "@types/node": "20.17.30",
    "@types/supertest": "6.0.3",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "9.1.7",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-node": "10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  }
}
