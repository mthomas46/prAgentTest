{
  "name": "balder",
  "version": "1.0.0",
  "description": "Service discovery and API gateway",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc && elm make src/Frontpage.elm --output=dist/public/elm.js",
    "dev": "concurrently \"npm run watch:elm\" \"npm run watch:ts\" \"nodemon dist/server.js\"",
    "watch:elm": "chokidar \"src/*.elm\" -c \"elm make src/Frontpage.elm --output=dist/public/elm.js --debug\"",
    "watch:ts": "tsc -w",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/cors": "^2.8.17",
    "@types/yamljs": "^0.2.34",
    "axios": "^1.6.7",
    "concurrently": "^8.0.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "swagger-ui-express": "^4.6.2",
    "typescript": "^5.0.3",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "chokidar-cli": "^3.0.0",
    "nodemon": "^2.0.22"
  },
  "engines": {
    "node": ">=18.0.0"
  },
