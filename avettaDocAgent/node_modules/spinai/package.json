{
  "name": "spinai",
  "version": "2.0.15",
  "license": "MIT",
  "author": "FallomAI",
  "description": "A lightweight framework for building AI agents",
  "repository": {
    "type": "git",
    "url": "https://github.com/Fallomai/spinai",
    "directory": "packages/spinai"
  },
  "bugs": {
    "url": "https://github.com/Fallomai/spinai/issues"
  },
  "homepage": "https://github.com/Fallomai/spinai#readme",
  "keywords": [
    "ai",
    "orchestration",
    "typescript"
  ],
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --treeshake --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint \"src/**/*.ts*\"",
    "test": "jest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.33.1",
    "@aws-sdk/client-bedrock-runtime": "^3.744.0",
    "@aws-sdk/credential-providers": "^3.749.0",
    "@google/generative-ai": "^0.21.0",
    "ajv": "^8.17.1",
    "fast-uri": "^3.0.6",
    "openai": "^4.77.0",
    "tsup": "^8.3.5",
    "uuid": "^9.0.0",
    "@modelcontextprotocol/sdk": "^1.6.1"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/uuid": "^9.0.7",
    "@types/shelljs": "^0.8.15",
    "typescript": "^5.3.3"
  }
}
